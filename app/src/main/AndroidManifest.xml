<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.harinijanani.familytree">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.harinijanani.familytree.AddPerson"></activity>
        <activity android:name="com.harinijanani.familytree.FamilyListView"></activity>
        <activity android:name="com.harinijanani.familytree.AddRelatives"></activity>
        <activity android:name="com.harinijanani.familytree.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.harinijanani.familytree.CreateFamily"
            android:label="@string/app_name" />
        <activity android:name="com.harinijanani.familytree.LoadFamily" />
        <activity android:name="com.harinijanani.familytree.UpdateFamily" />
        <activity android:name="com.harinijanani.familytree.ConfigureActivity" />


    </application>

</manifest>